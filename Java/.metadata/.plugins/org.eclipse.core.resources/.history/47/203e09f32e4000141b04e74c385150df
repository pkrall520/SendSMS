package Phillip.Krall.SMSMailer;

public enum Senders {
	
		T_MOBILE ("@tmomail.net"),
		VIRGIN_MOBILE (""),
		CINGULAR (""),
		SPRINT (""),
		VERIZON ("");
		
	private String email;
		
	Senders(String emailAddress){
		this.setEmail(emailAddress);
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}
}
